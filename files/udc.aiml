<?xml version="1.0" encoding="UTF-8"?>
<aiml>
   <category>
      <pattern>*</pattern>
      <template>
         <!--<delay>1.5</delay>-->
         <random>
            <li>I'm not sure how to respond to that.</li>
            <li>I don't know what you mean.</li>
         </random>
      </template>
   </category>

   <category>
      <pattern>Greetings</pattern>
      <template>
         <random>
            <li>Hello</li>
            <li>Hi</li>
            <li>/waves</li>
            <li>Greetings</li>
         </random>
      </template>
   </category>

   <category>
      <pattern>Hi</pattern>
      <template>
         <srai>Greetings</srai>
